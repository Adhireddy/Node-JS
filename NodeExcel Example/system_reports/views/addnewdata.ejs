<div  class="col-md-12">
<%if (editdata.length > 0) { %>
<input type="hidden" class="form-control" id="field_edit_data" value="<%= editdata %>" name="field_edit_data[]">
<% } %>

	  <form class="form-horizontal" id="adddataform" method="post" action="">
						<% supplies.forEach(function (data) { %>
						    <div class="form-group">
						  <label class="control-label col-sm-3" for="pwd"><%= data.caption_name %></label>
						  <div class="col-sm-6">          
							<input type="text" class="form-control" id="<%= data.field_name %>"   name="fielddata[]">
							<input type="hidden" class="form-control"  value="<%= data.field_name %>" name="field_name[]">
							<input type="hidden" class="form-control" value="<%= data.table_name %>" name="table_name">
							<input type="hidden" class="form-control"  value="<%= data.table_id %>" name="table_id">
							<input type="hidden" class="form-control"  value="<%= data.id %>"  name="row_id">
						  </div>
						</div>
        <% }) %>
		

				

						<div class="form-group ">
							<button type="button" id="" class="btn btn-primary data_submit login-button">Add data</button>
						</div>
		</form>
	</div>
	     <table class="table table-bordered">

  </table>  
<script type='text/javascript'>
        $(function(){
		
		var edit_data=$('#field_edit_data').val();
		if (typeof edit_data != 'undefined')
			{
				var data = jQuery.parseJSON(edit_data.replace(/\n/g,"\\n"));
				var sub_key=[];
				var sub_val=[];
				var field_name=[];

				for(var i in data){
					var key = i;
					var val = data[i];
					for(var j in val){
				
					   $(val).each(function(){
							$('#'+j).val((val[j]));
							 $("button").removeClass("data_submit login-button").addClass("data_edit edit-button");
							 $("button").html('Update Data');

						});	
						
					}
				}
			}
			
		});
     </script>
